{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://grs.com/schemas/edge",
	"title": "Graph Edge Schema",
	"type": "object",
	"additionalProperties": false,
	"properties": {
		"key": {
			"type": "string"
		},
		"source": {
			"type": "string"
		},
		"target": {
			"type": "string"
		},
		"attributes": {
			"type": "object",
			"properties": {
				"type": {
					"type": "string"
				}
			},
			"additionalProperties": true
		}
	},
	"required": ["key", "source", "target", "attributes"]
}
